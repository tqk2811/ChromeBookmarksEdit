{
    "name": "Bookmarks edit",
    "description": "This is an example of a Google Chrome extension.",
    "version": "0.0.0.1",
    "permissions": [ "bookmarks" ],
    "background": {
        "scripts": [ "background.js" ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "img/icon.png"
    },
	"options_page": "options.html",
    "manifest_version": 2
}